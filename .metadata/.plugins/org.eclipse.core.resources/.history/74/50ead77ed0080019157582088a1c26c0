		return thisPeriodDepreciationAmount;
	}

	public void setThisPeriodDepreciationAmount(BigDecimal thisPeriodDepreciationAmount) {
		this.thisPeriodDepreciationAmount = thisPeriodDepreciationAmount;
	}

	public BigDecimal getThisYearDepreciationAmount() {
		return thisYearDepreciationAmount;
	}

	public void setThisYearDepreciationAmount(BigDecimal thisYearDepreciationAmount) {
		this.thisYearDepreciationAmount = thisYearDepreciationAmount;
