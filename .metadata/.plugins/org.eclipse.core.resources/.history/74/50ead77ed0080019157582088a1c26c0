public class FaDepreciationSummaryTableVO {

	/** 编号 **/
	private Integer id;
	/** 资产编号 **/
	private String assetCode;
	/** 资产名称 **/
	private String assetName;
	/** 型号 **/
	private String model;
	/** 会计年度 **/
	private Integer accountYear;
